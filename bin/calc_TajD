#!/usr/bin/env python

"""A command line wrapper for calculation of Tajima's D."""


from selectiontest import selectiontest
import click

@click.command()
@click.argument('sfs')
def main(sfs):
    sfs = sfs.split(',')
    sfs = [int(x) for x in sfs]
    print(selectiontest.calculate_D(sfs))


if __name__ == "__main__":
    main()